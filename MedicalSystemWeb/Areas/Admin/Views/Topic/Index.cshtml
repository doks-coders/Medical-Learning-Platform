
<div class="row pt-4">
    <div class="col-6">
        <h3 class="text-primary"> Topics</h3>
    </div>
    <div class="col-6 text-end">
        <a asp-controller="Topic" asp-area="Admin" asp-action="Upsert" class="btn btn-primary">
            <i class="bi bi-plus-circle"></i> &nbsp;
            Upload New Topic
        </a>
    </div>

    <table data-bs-theme="dark" id="tblData" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Title</th>
                <th>Course</th>
                <th>Lecture Date</th>

                <th></th>
            </tr>
        </thead>
    </table>
</div>




@section Scripts {
    <script src="~/js/topic.js"></script>

}




